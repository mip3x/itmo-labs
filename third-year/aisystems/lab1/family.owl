  <!-- =================== SWRL ПРАВИЛА =================== -->

  <!-- father(F,C) :- parent(F,C), Male(F)  ==> fatherOf(F,C) -->
  <swrl:Imp>
    <rdfs:label>fatherOf rule</rdfs:label>
    <swrl:body rdf:parseType="Collection">
      <swrl:IndividualPropertyAtom>
        <swrl:propertyPredicate rdf:resource="#parentOf"/>
        <swrl:argument1 rdf:resource="#F"/>
        <swrl:argument2 rdf:resource="#C"/>
      </swrl:IndividualPropertyAtom>
      <swrl:ClassAtom>
        <swrl:classPredicate rdf:resource="#Male"/>
        <swrl:argument1 rdf:resource="#F"/>
      </swrl:ClassAtom>
    </swrl:body>
    <swrl:head rdf:parseType="Collection">
      <swrl:IndividualPropertyAtom>
        <swrl:propertyPredicate rdf:resource="#fatherOf"/>
        <swrl:argument1 rdf:resource="#F"/>
        <swrl:argument2 rdf:resource="#C"/>
      </swrl:IndividualPropertyAtom>
    </swrl:head>
  </swrl:Imp>

  <!-- mother(M,C) :- parent(M,C), Female(M)  ==> motherOf(M,C) -->
  <swrl:Imp>
    <rdfs:label>motherOf rule</rdfs:label>
    <swrl:body rdf:parseType="Collection">
      <swrl:IndividualPropertyAtom>
        <swrl:propertyPredicate rdf:resource="#parentOf"/>
        <swrl:argument1 rdf:resource="#M"/>
        <swrl:argument2 rdf:resource="#C"/>
      </swrl:IndividualPropertyAtom>
      <swrl:ClassAtom>
        <swrl:classPredicate rdf:resource="#Female"/>
        <swrl:argument1 rdf:resource="#M"/>
      </swrl:ClassAtom>
    </swrl:body>
    <swrl:head rdf:parseType="Collection">
      <swrl:IndividualPropertyAtom>
        <swrl:propertyPredicate rdf:resource="#motherOf"/>
        <swrl:argument1 rdf:resource="#M"/>
        <swrl:argument2 rdf:resource="#C"/>
      </swrl:IndividualPropertyAtom>
    </swrl:head>
  </swrl:Imp>

  <!-- sibling(A,B) :- parent(P,A), parent(P,B), A != B  ==> siblingOf(A,B) -->
  <swrl:Imp>
    <rdfs:label>siblingOf rule</rdfs:label>
    <swrl:body rdf:parseType="Collection">
      <swrl:IndividualPropertyAtom>
        <swrl:propertyPredicate rdf:resource="#parentOf"/>
        <swrl:argument1 rdf:resource="#P"/>
        <swrl:argument2 rdf:resource="#A"/>
      </swrl:IndividualPropertyAtom>
      <swrl:IndividualPropertyAtom>
        <swrl:propertyPredicate rdf:resource="#parentOf"/>
        <swrl:argument1 rdf:resource="#P"/>
        <swrl:argument2 rdf:resource="#B"/>
      </swrl:IndividualPropertyAtom>
      <swrl:DifferentIndividualsAtom>
        <swrl:argument1 rdf:resource="#A"/>
        <swrl:argument2 rdf:resource="#B"/>
      </swrl:DifferentIndividualsAtom>
    </swrl:body>
    <swrl:head rdf:parseType="Collection">
      <swrl:IndividualPropertyAtom>
        <swrl:propertyPredicate rdf:resource="#siblingOf"/>
        <swrl:argument1 rdf:resource="#A"/>
        <swrl:argument2 rdf:resource="#B"/>
      </swrl:IndividualPropertyAtom>
    </swrl:head>
  </swrl:Imp>

  <!-- grandparent(G,C) :- parent(G,P), parent(P,C)  ==> grandparentOf(G,C) -->
  <swrl:Imp>
    <rdfs:label>grandparentOf rule</rdfs:label>
    <swrl:body rdf:parseType="Collection">
      <swrl:IndividualPropertyAtom>
        <swrl:propertyPredicate rdf:resource="#parentOf"/>
        <swrl:argument1 rdf:resource="#G"/>
        <swrl:argument2 rdf:resource="#P"/>
      </swrl:IndividualPropertyAtom>
      <swrl:IndividualPropertyAtom>
        <swrl:propertyPredicate rdf:resource="#parentOf"/>
        <swrl:argument1 rdf:resource="#P"/>
        <swrl:argument2 rdf:resource="#C"/>
      </swrl:IndividualPropertyAtom>
    </swrl:body>
    <swrl:head rdf:parseType="Collection">
      <swrl:IndividualPropertyAtom>
        <swrl:propertyPredicate rdf:resource="#grandparentOf"/>
        <swrl:argument1 rdf:resource="#G"/>
        <swrl:argument2 rdf:resource="#C"/>
      </swrl:IndividualPropertyAtom>
    </swrl:head>
  </swrl:Imp>

</rdf:RDF>

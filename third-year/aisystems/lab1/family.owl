    xml:base="http://example.org/family"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#">

  <owl:Ontology rdf:about="http://example.org/family"/>

  <!-- ===================== КЛАССЫ ===================== -->
  <owl:Class rdf:about="#Person"/>
  <owl:Class rdf:about="#Male">
    <rdfs:subClassOf rdf:resource="#Person"/>
  </owl:Class>
  <owl:Class rdf:about="#Female">
    <rdfs:subClassOf rdf:resource="#Person"/>
  </owl:Class>

  <!-- Рефикация 3-местных отношений married/3 и divorced/3 -->
  <owl:Class rdf:about="#Marriage"/>
  <owl:Class rdf:about="#Divorce"/>

  <!-- ================= ОБЪЕКТНЫЕ СВОЙСТВА ================= -->
  <owl:ObjectProperty rdf:about="#parentOf">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#childOf">
    <owl:inverseOf rdf:resource="#parentOf"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#spouseOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>

  <!-- Связи к событиям Marriage/Divorce -->
  <owl:ObjectProperty rdf:about="#spouse1">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="#Marriage"/>
          <rdf:Description rdf:about="#Divorce"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#spouse2">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="#Marriage"/>
          <rdf:Description rdf:about="#Divorce"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>

  <!-- Выводимые свойства -->
  <owl:ObjectProperty rdf:about="#fatherOf">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#motherOf">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#siblingOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#grandparentOf">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>

  <!-- ================== ДАТА-СВОЙСТВА ================== -->
  <owl:DatatypeProperty rdf:about="#hasName">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#birthYear">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#deathYear">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#eventYear">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="#Marriage"/>
          <rdf:Description rdf:about="#Divorce"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>

